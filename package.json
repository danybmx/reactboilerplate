{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "A simple react boileplate for use in my projects",
  "scripts": {
    "start": "nodemon --exec babel-node ./tools/server.js --ignore client",
    "build": "NODE_ENV=production babel-node ./tools/build.js",
    "serve": "NODE_ENV=production babel-node ./tools/server.js",
    "test": "jest"
  },
  "jest": {
    "rootDir": "./src",
    "scriptPreprocessor": "../preprocessor.js",
    "unmockedModulePathPatterns": [
      "fbjs",
      "react"
    ]
  },
  "keywords": [
    "react",
    "boilerplate"
  ],
  "author": "Daniel Rogi <daniel@dpstudios.es>",
  "license": "MIT",
  "dependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel-core": "^6.4.0",
    "express": "^4.13.3",
    "express-handlebars": "^2.0.1",
    "history": "^1.17.0",
    "hjs": "0.0.6",
    "isomorphic-fetch": "^2.2.0",
    "radium": "^0.16.1",
    "react": "^0.14.6",
    "react-document-meta": "^2.0.1",
    "react-dom": "^0.14.6",
    "react-redux": "^4.0.6",
    "react-router": "^1.0.3",
    "redux": "^3.0.5",
    "redux-logger": "^2.3.1",
    "redux-thunk": "^1.0.3",
    "socket.io": "^1.4.4",
    "socket.io-client": "^1.4.4"
  },
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.1",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^3.0.2",
    "eslint-loader": "^1.2.0",
    "eslint-plugin-react": "^3.14.0",
    "jest-cli": "^0.8.2",
    "jscs": "^2.8.0",
    "mkdirp": "^0.5.1",
    "nodemon": "^1.8.1",
    "react-transform-catch-errors": "^1.0.1",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.2.0",
    "redux-devtools": "^3.0.1",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.1",
    "webpack": "^1.12.10",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
  }
}
