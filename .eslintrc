{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "globals": {
    "__DEVELOPMENT__": true,
    "_store": true,
    "_history": true
  },
  "rules": {
    "no-shadow": 0,
    "react/jsx-quotes": 0,
    "jsx-quotes": [2, "prefer-double"],
    "react/jsx-no-bind": 0,
    "react/jsx-closing-bracket-location": 0
  }
}
